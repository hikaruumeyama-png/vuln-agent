# ===========================================
# Gmail設定
# ===========================================

# 【個人Gmail用】OAuthトークン（setup_gmail_oauth.py で自動設定）
# GMAIL_OAUTH_TOKEN=（Base64エンコードされたトークン）

# 【Google Workspace用】ドメイン委任でアクセスするユーザーのメールアドレス
# GMAIL_USER_EMAIL=your-email@example.com

# SIDfmの送信元メールアドレス
SIDFM_SENDER_EMAIL=noreply@sidfm.com

# ===========================================
# SBOM設定（スプレッドシート）
# ===========================================

# スプレッドシートID（URLから取得: https://docs.google.com/spreadsheets/d/【この部分】/edit）
SBOM_SPREADSHEET_ID=your-spreadsheet-id

# シート名
SBOM_SHEET_NAME=SBOM
OWNER_SHEET_NAME=担当者マッピング

# ===========================================
# Google Chat設定
# ===========================================

# 通知先スペースID（spaces/XXXXX形式）
DEFAULT_CHAT_SPACE_ID=spaces/your-space-id

# ===========================================
# GCP設定（オプション）
# ===========================================

# プロジェクトID（通常は自動検出）
# GCP_PROJECT_ID=your-project-id

# リージョン
# GCP_LOCATION=asia-northeast1
